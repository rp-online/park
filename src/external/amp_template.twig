    <script async custom-element="amp-consent" src="https://cdn.ampproject.org/v0/amp-consent-0.1.js"></script>
    <script async custom-element="amp-geo" src="https://cdn.ampproject.org/v0/amp-geo-0.1.js"></script>
  {% endif %}

  <link rel="icon" href="{{ siteurl }}{{ favicon }}" sizes="16x16 32x32 48x48" type="image/x-icon">

  {% block head_appendChildren %} {% endblock %}
</head>
<body>
{% if consentActive %}
  <amp-geo layout="nodisplay">
    <script type="application/json">
      {
        "ISOCountryGroups": {
          "eu": [
            "al", "ad", "am", "at", "by", "be", "ba", "bg", "ch", "cy",
            "cz", "de", "dk", "ee", "es", "fo", "fi", "fr", "gb", "ge",
            "gi", "gr", "hu", "hr", "ie", "is", "it", "lt", "lu", "lv",
            "mc", "mk", "mt", "no", "nl", "po", "pt", "ro", "ru", "se",
            "si", "sk", "sm", "tr", "ua", "uk", "va"
          ]
        }
      }
    </script>
  </amp-geo>
  <amp-consent layout="nodisplay" id="consent-element">
    <script type="application/json">
      {
        "consents": {
          "de": {
            "promptIfUnknownForGeoGroup": "eu",
            "promptUI": "myConsentFlow"
          }
        },
        "postPromptUI": "post-consent-ui"
      }
    </script>
    <div id="myConsentFlow" class="popupOverlay">
      <div class="consentPopup">
        <amp-iframe id='idFrame' layout="fill" resizable sandbox="allow-scripts allow-same-origin allow-popups allow-popups-to-escape-sandbox" src="{{ htmlPageUrl }}">
          <div placeholder></div>
          <div overflow></div>
        </amp-iframe>
      </div>
    </div>
    <div id="post-consent-ui">
      <a on="tap:consent-element.prompt;" role="button" class="manage-settings-link"></a>
    </div>
  </amp-consent>
{% endif %}
{% block body_prependChildren %} {% endblock %}
<amp-sidebar id="sidebar"
             layout="nodisplay"
             side="right"
             class="sidebar">
  <h6>Themen</h6>
  <ul>
    {% for element in navigation %}
      <li><a href="{{ element.href }}">{{ element.text }}</a></li>
    {% endfor %}
  </ul>
</amp-sidebar>
<amp-analytics type="infonline" id="infonline">
  <script type="application/json">
    {
      "vars": {
        "st": "{{ ivw_mobil_st }}",
        "co": "{{ baseurl_canonical }}",
        "cp": "{{ ivw.cp }}"
      },
      "requests": {
        "url": "{{ ivw_requests_url }}"
      }
    }
  </script>
</amp-analytics>
{% if gtm_id is defined and gtm_id != '' and gtm_htmlfile != '' %}
  <amp-analytics data-block-on-consent="_till_responded" type="googleanalytics" id="googleanalytics">
    <script type="application/json">
      {
        "extraUrlParams": {{ extraUrlParams|raw }},
        "vars": {
          "account": "{{ gtm_id }}"
        },
        "triggers": {
            "trackPageview": {
                "on": "visible",
                "request": "pageview"
            }
        }
      }
    </script>
  </amp-analytics>
{% endif %}

{% if linkpulse is defined and linkpulse != '' %}
  {% set section = section != '' ? section : (config.dataLayer.dossier is defined ? config.dataLayer.dossier : config.dataLayer.ressort) %}
  <amp-analytics type="linkpulse">
    <script type="application/json">
      {
        "vars": {
          "id"      : "{{ linkpulse }}",
          "pageUrl" : "{{ baseurl_full }}",
          "title"   : {{ pagetitle|json_encode()|raw }},
          "section" : {{ section|json_encode()|raw }},
          "channel" : "amp",
          "type"    : "{{ contentType_raw }}",
          "image"   : "{{ siteurl }}{{ metaImage.src }}"
        }
      }
    </script>
  </amp-analytics>
{% endif %}

{% if gtm_id is not defined and analytics1 is defined and analytics1 != '' %}
  <amp-analytics data-block-on-consent="_till_responded" type="googleanalytics" id="analytics1">
    <script type="application/json">
      {
        "vars": {
          "account": "{{ analytics1 }}"
        },
        "extraUrlParams": {
          "cd10": "AMP"
        },
        "triggers": {
          "trackPageviewWithCustomData": {
            "on": "visible",
            "request": "pageview",
            "vars": {
              "title": {{ pagetitle|json_encode()|raw }},
              "documentLocation": "{{ baseurl_full }}"
            }
          }
        }
      }
    </script>
  </amp-analytics>
{% endif %}
{% if gtm_id is not defined and analytics2 is defined and analytics2 != '' %}
  <amp-analytics data-block-on-consent="_till_responded" type="googleanalytics" id="analytics2">
    <script type="application/json">
      {
        "vars": {
          "account": "{{ analytics2 }}"
        },
        "extraUrlParams": {
          "cd10": "AMP"
        },
        "triggers": {
          "trackPageviewWithCustomData": {
            "on": "visible",
            "request": "pageview",
            "vars": {
              "title": {{ pagetitle|json_encode()|raw }},
              "documentLocation": "{{ baseurl_full }}"
            }
          }
        }
      }
    </script>
  </amp-analytics>
{% endif %}
{% if vgwort_ic_oeffentlich != '' %}
  <amp-pixel src="{{ vgwort_pixel_url }}" layout="nodisplay"></amp-pixel>
{% endif %}

{% block main_header %}
  <header class="amp-header">
    <a href="{{ siteurl }}/?utm_source=Google&utm_medium=amp&utm_campaign=header" target=_blank class='amp-header-oe'><span class="amp-header-logo"></span></a>
    {% if activatePartnerLink %}
    <a href="{{ partner_link }}" class="amp-header-partner"><amp-img src="{{ partner_logo }}" alt="Partner von RP ONLINE" width="134" height="17" /></a>
    {% endif %}
    <div role="button" on="tap:sidebar.toggle" tabindex="0" class="hamburger amp-header-menuswitch">☰</div>
  </header>
{% endblock %}
<main>

  {% if breadcrumb is defined %}
  {% set count = 0 %}
  <div class="park-section-breadcrumb">
    <div class="park-section-breadcrumb__wrapper">
        <ol itemscope itemtype="http://schema.org/BreadcrumbList" class="park-section-breadcrumb__navigation">
          {% for item in breadcrumb.navigation %}
            {% if item.isAd is not defined or item.isAd is not same as(true) or config.disableAds is not defined or not config.disableAds is same as(true) %}
              {% set ariaHidden = item.isHidden ? 'true' : 'false' %}
              {% set desktopHiddenClass = item.isDesktopHidden ? 'park-section-breadcrumb__item--desktophidden' : '' %}
              {% if item.text is not empty %}
                <li itemprop="itemListElement"
                    itemscope
                    itemtype="http://schema.org/ListItem"
                    class="park-section-breadcrumb__item {{ desktopHiddenClass }}"
                    aria-hidden="{{ ariaHidden }}">
                  {% set content %}
                    <span itemprop="name">{{ item.text | raw }}</span>
                  {% endset %}
                  {% set link = {
                    'itemprop': 'item',
                    'href': item.href,
                    'text': content,
                    'target': item.target,
                    'rel': item.rel,
                    'classes': [
                      'park-section-breadcrumb__link',
                      item.isAd ? 'park-section-breadcrumb__link--sugardaddy' : ''
                    ],
                    'isAd': item.isAd,
                    'config': config
                  } %}
                  {% include 'components/link-helper/template.twig' with link only %}
                  {% set count = count + 1 %}
                  <meta itemprop="position" content="{{ count }}" />
                </li>
              {% endif %}
            {% endif %}
          {% endfor %}
        </ol>
      <{{ breadcrumb.headline.tag }} class="park-section-breadcrumb__headline">{{ breadcrumb.headline.text | raw }}</{{ breadcrumb.headline.tag }}>
  </div>
  </div>
  {% endif %}

  {% block pre_article %} {% endblock %}
  <article>
    <header>
      <time class="date">{{ datum | date('j. F Y', 'Europe/Berlin')|replace({
          'January': 'Januar',
          'February': 'Februar',
          'March': 'März',
          'April': 'April',
          'May': 'Mai',
          'June': 'Juni',
          'July': 'Juli',
          'August': 'August',
          'September': 'September',
          'October': 'Oktober',
          'November': 'November',
          'December': 'Dezember'
        }) }} um {{ datum|date('H:i', 'Europe/Berlin') }} Uhr</time>
      <strong class="kicker">{{ dachzeile | raw }}</strong>
      <h1 class="articleheadline">{{ titel | raw }}</h1>
    </header>
    <amp-image-lightbox id="lightbox1" layout="nodisplay"></amp-image-lightbox>

    {% if coverHtml != null %}{{ coverHtml|raw }}{% endif %}

    <div class="share-buttons">
      {% if fb_app_id %}
        <amp-social-share type="facebook" data-param-app_id="{{ fb_app_id }}"></amp-social-share>
      {% endif %}
      <amp-social-share type="whatsapp"></amp-social-share>
      <amp-social-share type="twitter"></amp-social-share>
      <amp-social-share type="email"></amp-social-share>
    </div>

    <p class="entrySummary">
      {% if artikelmarkierung != '' %}<span class="exclusiv">{{ artikelmarkierung }}</span>{% endif %}
      {% if ortsmarke != '' %}<span class="location">{{ ortsmarke }}</span>{% endif %}
      {{ vorspann | raw }}
    </p>

    {% if authorsbox is defined and authorsbox.text is defined %}
      {% set authorsbox = authorsbox|merge({ 'view': 'amp' }) %}
      {% include 'components/authorsbox-amp/template.twig' with authorsbox only  %}

    {% else %}
      {% if credittext != '' %}
        <p class="author">
          <span itemprop="author">{{ credittext }}</span>
        </p>
      {% endif %}
    {% endif %}

    {% block body_article_bodyParagraphs %}
      {% set ampnewsroomshown = false %}
      {% for paragraph in body %}
        {{ paragraph.html|raw }}
        {% if ampnewsroom_url != '' and paragraph.subtype is defined and paragraph.subtype == 'headline' and ampnewsroomshown == false %}
          {% set ampnewsroomshown = true %}
          <amp-iframe width="1px" height="1px" layout="fixed" sandbox="allow-scripts allow-same-origin" src="{{ ampnewsroom_url }}"></amp-iframe>
        {% endif %}
      {% endfor %}
    {% endblock %}

    {% if meldung_agentur != '' %}
      <p>{{ meldung_agentur }}</p>
    {% endif %}

    {% if tickaroo_id != '' %}
      <amp-iframe
        width="50"
        height="200"
        layout="responsive"
        sandbox="allow-scripts allow-same-origin allow-popups"
        allowfullscreen
        resizable
        src="https://cdn.tickaroo.com/embed/amp.html?client_id={{ tickaroo_client_id }}&id={{ tickaroo_id }}&style={% if tickaroo_kachelformat %}imagegrid{% else %}v2{% endif %}&showEventSharing={% if tickaroo_sharing %}true{% else %}false{% endif %}&coalesceRepeatedMeta={% if tickaroo_autorenbilder_zusammenfassen %}true{% else %}false{% endif %}&reverseEvents={% if tickaroo_beitragsreihenfolge_umkehren %}true{% else %}false{% endif %}&limit={{tickaroo_limit}}&showEventMeta=reporter">
        <amp-img layout="fixed-height" height="100px" width="auto"
                 src="https://cdn.tickaroo.com/embed/placeholder.png" placeholder>
        </amp-img>
        <div overflow="">Liveblog öffnen</div>
      </amp-iframe>
    {% endif %}

    {% block body_article_appendChildren %}
      {{ global_ad_config.werbesteuerung_amp.artikelende|raw }}
    {% endblock %}
  </article>

  {% block body_postArticle %} {% endblock %}

  <footer class="amp-footer"><a href="{{ siteurl }}/?utm_source=Google&utm_medium=amp&utm_campaign=footer" target=_blank><span class="amp-footer-logo"></span></a>
    <div>
      <a href="{{ link_imprint }}" target=_blank>Impressum</a>
    <span> |  </span>
      <a href="{{ link_tba }}" target=_blank>Datenschutz</a>
      {% if consentActive %}
        <span> |  </span>
        <a on="tap:consent-element.prompt;" role="button" class="manage-settings-link">Cookie Einstellungen</a>
      {% endif %}
    </div>
  </footer>
</main>
{% block body_appendChildren %} {% endblock %}
</body>
</html>
