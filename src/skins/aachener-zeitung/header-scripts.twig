<link rel="stylesheet" media="all" href="/assets/zva/styles.css">
<script type="text/javascript" src="/assets/zva/scripts.js"></script>

<!-- liveReporting | Wenn es nicht via Google Tagmanager gebucht wird. -->
<script>
    document.addEventListener('DOMContentLoaded', function() {
    if (
      !window.dataLayer ||
      !dataLayer.length ||
      (!dataLayer[0].rho_url && !dataLayer[0].rhoUrl)
    ) {
      return;
    }

    var rhoUrl = dataLayer[0].rho_url || dataLayer[0].rhoUrl;
    rhoUrl = eval("'" + rhoUrl.replace(/^'/, ''));

    try {
      var xhttp = new XMLHttpRequest();
      xhttp.open('GET', rhoUrl);
      xhttp.send();
    } catch(e) {}
  });

    //GERACM-2924
    document.addEventListener("DOMContentLoaded", function(event) {
        var style = document.createElement('style');
        style.textContent = '.park-embed-image .park-image.park-image { clip: auto }';
        document.head.appendChild(style);
    });
</script>