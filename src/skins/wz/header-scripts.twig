<!-- PaidTime -->
{% if config.disableAds is not defined or not config.disableAds is same as(true) %}
  <script type="text/javascript">
    var isClickDummy = (
      window.park.exports &&
      window.park.exports.config &&
      window.park.exports.config.isClickDummy
    );

    var wzMobile = window.location.href.indexOf('/mobile/') >= 0;
    var wzAB = (typeof jsABLoaded == 'undefined') || !(jsABLoaded); // GERACM-5963

    // Do not run paidtime when we build clickdummy pages
    if (typeof isClickDummy === "undefined" || !isClickDummy) {
      // PaidTime
      window.addEventListener('keydown', function (e) {
        if (e.keyCode == 27) {e.preventDefault()}
      });

      function init() {
        if (!document.body) {
          window.setTimeout(init, 100);
          return;
        }

        (function (t, i, m, e) {
          m = t.createElement(i);
          m.src = 'https://admin.paidtime.com/webclient/client.1000.js';
          m.id = 'pt-app-main';
          m.pid = 4195;
          m.cid = '';
          m.ab = wzAB;
          m.root = '.park-section-portal-top__content';
          m.yOffsetTop = wzMobile ? 250 : 400;
          m.yOffsetBottom = wzMobile ? 0 : 0;
          m.xOffset = 0;
          e = t.getElementsByTagName(i)[0];
          e.parentNode.insertBefore(m, e);
        }(document, 'script'));
      }
      init();
    }
  </script>
{% endif %}

